@model Employee

@{
    ViewData["Title"] = "Сотрудник #" + Model.Id;
}



<div class="container">
    <h3>@ViewData["Title"]</h3>
    <p>@Model.SurName</p>
    <p>@Model.FirstName</p>
    <p>@Model.Patronymic</p>
    <p>@Model.Age</p>

    <a asp-controller="Employees" asp-action="Index">Сотрудники</a>
</div>

